{
  "name": "grpc-web-simple-example",
  "version": "0.1.0",
  "description": "gRPC-Web simple example",
  "main": "server.js",
  "scripts": {
    "protoc": "protoc -I=. helloworld.proto --js_out=import_style=commonjs:. --grpc-web_out=import_style=commonjs,mode=grpcwebtext:.",
    "client": "npx webpack client.js",
    "server": "node server.js",
    "docker-build": "docker build -t helloworld .",
    "docker-run": "docker run -d --name helloworld -p 8080:8080 -p 9901:9901 helloworld",
    "webserver": "python -m http.server 8081"
  },
  "devDependencies": {
    "@grpc/grpc-js": "~1.1.8",
    "@grpc/proto-loader": "~0.5.4",
    "async": "~3.2.3",
    "google-protobuf": "~3.14.0",
    "grpc-web": "~1.3.1",
    "lodash": "~4.17.0",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.11"
  }
}
